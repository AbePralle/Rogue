class Method
  PROPERTIES
    t         : Token
    this_type : Type
    name      : String

    statements = CmdStatementList()

    resolved  : Logical

  METHODS
    method init( t, this_type, name )

    method print_prototype( writer:CPPWriter )
      writer.println( "TODO" )

    method resolve
      if (resolved) return
      resolved = true

      statements.resolve
endClass

