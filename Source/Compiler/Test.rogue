$include "Libraries/Standard"

class Test
  PROPERTIES

  METHODS
    method init
      local i = 0
      while (i <= 10)
        which (i)
          case 1
            println "1st"
          case 2
            println "2nd"
          case 3
            println "3rd"
            escapeWhile
          others
            println( i + "th" )
        endWhich
        ++i
      endWhile
      println "Done"
endClass

