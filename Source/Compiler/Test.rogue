class Test
  PROPERTIES

  METHODS
    method init
      local name = Pair<<Alpha,Beta>>( Alpha(), Beta() )
      name.first = Alpha()
      local a : Alpha
      a = name.first
      println "Woot!"

    method sqrt( n:Integer )->Integer
      $if ("C++") native '((int)sqrt((double)$n))'
endClass

class Alpha
  PROPERTIES
    a = 3
endClass

class Beta : Alpha
  PROPERTIES
    b = 4
endClass

class Gamma : Beta
  PROPERTIES
    c = 5
endClass

class Pair<<$FirstType:Object,$SecondType>>
  PROPERTIES
    first  : $FirstType
    second : $SecondType

  METHODS
    method init( one:$FirstType, two:$SecondType  )
endClass

primitive Integer
endPrimitive

primitive Logical
endPrimitive

primitive Real
endPrimitive

class Object
endClass

class String
endClass

class Array<<$DataType>> [native]
endClass

