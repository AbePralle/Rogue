$include "Libraries/Standard"

class Test
  METHODS
    method init
      task_test.start
      println "tasks started"

    method task_test [task]
      local x = await print( 1, 5 )
      local y = await print( -5, -1 )
      println "x:$ y:$" (x,y)

    method print( i1:Integer, i2:Integer )->Integer [task]
      forEach (n in i1..i2)
        println n
        yield
      endForEach
      return i2
endClass

