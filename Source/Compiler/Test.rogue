class Test
  PROPERTIES

  METHODS
    method init
      local a = Array<<Integer>>(5)
      a[0] = 2
      a[1] = a[0]++
      a[2] = ++a[0]
      println a[0]
      println a[1]
      println a[2]
      println a.count
      println "Woot!"

    method sqrt( n:Integer )->Integer
      $if ("C++") native '((int)sqrt((double)$n))'
endClass

class Alpha
  PROPERTIES
    a = 3
endClass

class Beta : Alpha
  PROPERTIES
    b = 4
endClass

class Gamma : Beta
  PROPERTIES
    c = 5
endClass

class Pair<<$FirstType:Object,$SecondType>>
  PROPERTIES
    first  : $FirstType
    second : $SecondType

  METHODS
    method init( one:$FirstType, two:$SecondType  )
endClass

primitive Integer
endPrimitive

primitive Logical
endPrimitive

primitive Real
endPrimitive

class Object
endClass

class String
endClass

class NativeArray [native]
endClass

class Array<<$DataType>> : NativeArray [native]
  METHODS
    method count->Integer [native]
endClass

