class Test
  PROPERTIES

  METHODS
    method init
      println "Prime numbers between 1 and 100:"
      forEach (n in 1..100)
        if (is_prime(n)) println n
      endForEach

    method is_prime( n:Integer )->Logical
      if (n <= 1) return false
      if (n <= 3) return true
      forEach (denom in 2..n-1)
        if (n % denom == 0) return false
      endForEach
      return true
endClass

