$include "Libraries/Standard"

class Test
  PROPERTIES

  METHODS
    method init
      print_result.start

    method print_result [task]
      println await task_test

    method task_test->Integer [task]
      forEach (n in 1..10)
        which (n)
          case 1: println "1st"
          case 2: println "2nd"
          case 3: yield; println "3rd"; return 5
          others: println n + "th"
        endWhich
      endForEach
      return 6


