$include "Libraries/Standard"

class PointTest
  METHODS
    method init
      local pt1 = Point2D(3,4)
      local pt2 = Point3D(5,6,7)
      println pt1  # prints (3,4)
      println pt2  # prints (5,6,7)
endClass

class Point2D( x:Integer, y:Integer )
  METHODS
    method to->String
      return "($,$)" (x,y)
endClass

class Point3D( x, y, z:Integer ) : Point2D
  METHODS
    method to->String
      return "($,$,$)" (x,y,z)
endClass
