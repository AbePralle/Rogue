module ParseKit

class Cmd<<$Language>>( t:Token<<$Language>> )
endClass

#{
class CmdBinaryOp<<$Language>>( t, left:Cmd, right:Cmd ) : Cmd<<$Language>> [abstract]
  METHODS
    method op->String [abstract]

    method to->String
      return "($ $ $)" (left,op,right)
endClass

class CmdAdd : CmdBinaryOp
  METHODS
    method op->String
      return "+"
endClass

class CmdLiteralInt32( t, value:Int32 ) : Cmd
  METHODS
    method to->String
      return value->String
endClass
}#
