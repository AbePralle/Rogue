module ParseKit

class Cmd<<$Language>>( t:Token<<$Language>> )
endClass

class CmdUnary<<$Language>>( t, operand:Cmd ) : Cmd<<$Language>> [abstract]
  METHODS
    method op->String [abstract]

    method to->String
      return "($$)" (op,left)
endClass

class CmdBinary<<$Language>>( t, left:Cmd<<$Language>>, right:Cmd<<$Language>> ) : Cmd<<$Language>> [abstract]
  METHODS
    method op->String [abstract]

    method to->String
      return "($ $ $)" (left,op,right)
endClass

