class XY( x:Real64, y:Real64 )
  METHODS
    method to->String
      return StringBuilder().print('(').print(x,2).print(',').print(y,2).print(')')->String
endClass

class Anchor( position:XY ) [compound]
  GLOBAL PROPERTIES
    LEFT   = Anchor( XY(-1, 0) )
    CENTER = Anchor( XY( 0, 0) )

  METHODS
    method to->String
      return position->String
endClass

println Anchor.LEFT
println Anchor.CENTER
