class Anchor( type:Int32 ) [compound]
  ENUMERATE
    ALIGN_LEFT     = 0b0000
    ALIGN_HCENTER  = 0b0001
    ALIGN_RIGHT    = 0b0010
    ALIGN_TOP      = 0b0000
    ALIGN_VCENTER  = 0b0100
    ALIGN_BOTTOM   = 0b1000
    ALIGN_ABSOLUTE = 0b1111

  GLOBAL PROPERTIES
    LEFT   = Anchor( ALIGN_LEFT )
    CENTER = Anchor( ALIGN_HCENTER | ALIGN_VCENTER )

  METHODS
    method to->String
      return type
endClass

println Anchor.LEFT
println Anchor.CENTER
