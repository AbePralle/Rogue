class Test
  METHODS
    method init
      print_long( 1->Long :<<: 63 )
      println
      print_long( (1->Long :<<: 63) - 1 )
      println

    method print_long( value:Long )->this
      if (value == (1->Long :<<: 63))
        print( "-9223372036854775808" )
      elseIf (value < 0)
        print( '-' )
        print_long( -value )
      elseIf (value >= 10)
        print_long( value / 10 )
        print( ('0'+(value%10))->Character )
      else
        print( ('0'+value)->Character )
      endIf
      return this
endClass

Test()
